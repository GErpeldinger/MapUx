{
  "name": "@gerpeldinger/mapux",
  "version": "1.0.0",
  "license": "MIT",
  "symfony": {
    "controllers": {
      "google-maps": {
        "main": "dist/google-maps-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true
      },
      "leaflet": {
        "main": "dist/leaflet-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "leaflet/dist/leaflet.css": true
        }
      },
      "mapbox": {
        "main": "dist/mapbox-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "mapbox-gl/dist/mapbox-gl.css": true
        }
      },
      "open-layers": {
        "main": "dist/open-layers-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "ol/ol.css": true
        }
      }
    }
  },
  "scripts": {
    "build": "babel src -d dist"
  },
  "peerDependencies": {
    "@googlemaps/js-api-loader": "^1.12.1",
    "leaflet": "^1.7.1",
    "mapbox-gl": "^2.3.1",
    "ol": "^6.6.1",
    "stimulus": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@googlemaps/js-api-loader": "^1.12.1",
    "leaflet": "^1.7.1",
    "mapbox-gl": "^2.3.1",
    "ol": "^6.6.1",
    "stimulus": "^2.0.0"
  }
}
