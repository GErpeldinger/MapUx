{
  "name": "@gerpeldinger/mapux",
  "description": "Map dependencies integration for Symfony",
  "version": "1.0.0",
  "license": "MIT",
  "symfony": {
    "controllers": {
      "google-maps": {
        "main": "dist/google-maps-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true
      },
      "leaflet": {
        "main": "dist/leaflet-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "leaflet/dist/leaflet.css": true
        }
      },
      "mapbox": {
        "main": "dist/mapbox-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "mapbox-gl/dist/mapbox-gl.css": true
        }
      },
      "open-layers": {
        "main": "dist/open-layers-controller.js",
        "webpackMode": "eager",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "ol/ol.css": true
        }
      }
    }
  },
  "peerDependencies": {
    "@googlemaps/js-api-loader": "^1.12.1",
    "@hotwire/stimulus": "^3.0.0",
    "leaflet": "^1.7.8",
    "mapbox-gl": "^2.6.0",
    "ol": "^6.6.1"
  },
  "devDependencies": {
    "@googlemaps/js-api-loader": "^1.12.1",
    "@hotwired/stimulus": "^3.0.0",
    "@types/google-maps": "^3.2.3",
    "@types/leaflet": "^1.7.8",
    "@types/mapbox-gl": "^2.6.0",
    "@types/ol": "^6.5.3",
    "leaflet": "^1.7.1",
    "mapbox-gl": "^2.3.1",
    "ol": "^6.6.1"
  },
  "dependencies": {
  }
}
